" Common {{{
" ClipBoard
set clipboard+=unnamed
set clipboard=unnamed
" }}}

" KeyMapping {{{
noremap ; :
noremap : ;

nnoremap <C-t> :<C-u>tabnew<CR>
nnoremap <C-l> gt
nnoremap <C-h> gT

nnoremap s <Nop>
nnoremap sj <C-w>j
nnoremap sk <C-w>k
nnoremap sl <C-w>l
nnoremap sh <C-w>h

nnoremap ss :<C-u>sp<CR>
nnoremap sv :<C-u>vs<CR>
nnoremap sq :<C-u>q<CR>
" }}}
