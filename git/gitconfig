[alias]
  s = status -sb
  l = log --decorate
  lp = log --decorate --stat -p
  ll = log --graph -n 20 --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(green)- %an, %cr%Creset'
  d = diff --histogram
  b = branch --verbose --verbose --color
  br = branch -avv
  info = remote show origin
  g = grep -n -p --color --heading --break
  co = checkout
  ad = add
  ci = commit -v
  fixup = commit --amend -C HEAD
  tb = git-tb
  bk = git-bk
  gsub = git-gsub
  see = browse
  n = now --all --stat
